{
  "name": "ca-wfc-v2",
  "version": "0.0.0",
  "description": "hackathon product for wfc2 v2",
  "author": "shooontan",
  "scripts": {
    "dev": "parcel src/index.html -p 3000",
    "build": "parcel build ./src/index.html --no-source-maps --detailed-report",
    "render-pages": "npm run build && node ./src/render-pages.js",
